                                                      title="{!tableList[0].thenoptionvalue}">
                                        <aura:iteration items="{!v.thenoptions}" var="option">
                                            <option text="{!option.label}" value="{!option.value}" />
                                        </aura:iteration>
                                    </lightning:select-->
                                    <!--div class="slds-text-longform">                                        
                                        <h3 class="slds-text-heading_small">{!tableList[0].thenoptionvalue}</h3>
                                    </div-->
                                    <lightning:input name="input5" disabled="true" title="{!tableList[0].thenoptionvalue}" value="{!tableList[0].thenoptionvalue}" variant="label-hidden" />
                                </div>
                            </div>
                            <aura:if isTrue="{!tableList[0].thenDirect}">
                                <div class="slds-col slds-p-left_medium  slds-size_3-of-12  " data-id="{!masterindex}">
                                    <lightning:input name="thenvalue" aura:id="thenDefault"
                                                     class="{!tableList[0].thenValueError}" variant="label-hidden"
