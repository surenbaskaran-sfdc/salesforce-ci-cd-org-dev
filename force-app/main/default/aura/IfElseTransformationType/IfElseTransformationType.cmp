            <div class="slds-page-header">
                <span class="slds-text-heading_small " title="Condition">
                    <b>Condition</b>
                </span>
            </div>
            <!--IF-->
            <aura:iteration items="{!v.masterTableList}" var="tableList" indexVar="masterindex">
                <div class="slds-grid slds-wrap ">
                    <div class=" slds-col slds-p-top_x-small slds-size_6-of-12 ">
                        <span class="slds-p-left_x-small" title="If">
                            <b>&nbsp;If</b>
                        </span>
                    </div>
                    <div class=" slds-col slds-size_6-of-12 ">
                        <div data-id="{!masterindex}" onclick="{! c.deleteif }">
                            <span class=" slds-float_right slds-p-right_xxx-small slds-text-heading_large">
                                <lightning:icon style="color:blue" iconName="utility:dash" title="Delete Condition"
                                                size="x-small" />
                            </span>
                        </div>
                        <div data-id="{!masterindex}" onclick="{! c.elsedecisionchange }">
                            <span class=" slds-float_right slds-p-right_x-small slds-text-heading_large">
                                <lightning:icon iconName="utility:add" title="Add Condition" size="x-small" />
                            </span>
                        </div>
                    </div>
