    <aura:registerEvent name="errorEvent" type="c:errorEvent" /> 
    <aura:registerEvent name="triggerSaveEvent" type="c:triggerSaveFieldsEvent" /> 
    
    <lightning:overlayLibrary aura:id="overlayLib" />
