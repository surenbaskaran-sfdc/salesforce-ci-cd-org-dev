                                        <lightning:input aura:id="box" style="width:314px;"
                                                         class="{!tableList[0].customConditionError}" variant="label-hidden" name="Name"
                                                         type="text" placeholder="1AND(2OR3)" value="{!tableList[0].customformula}"
                                                         title="{!tableList[0].customformula}" />
                                    </span>
                                </aura:if>
                                <span class="  slds-float_right slds-p-top_x-small slds-p-right_xx-small">
                                    <span style="color:black" data-id="{!masterindex}" data-value="All"
                                          onclick="{!c.checkBoolean}">
                                        <ui:inputRadio value="{!tableList[0].andCondition}" />
                                    </span>
                                    <span data-id="{!masterindex}" data-value="All" title="All"
                                          onclick="{!c.checkBoolean}">&nbsp;ALL &nbsp;&nbsp;&nbsp;</span>
                                    <span style="color:black" data-id="{!masterindex}" data-value="Any"
                                          onclick="{!c.checkBoolean}">
                                        <ui:inputRadio value="{!tableList[0].orCondition}" />
                                    </span>
                                    <span data-id="{!masterindex}" data-value="Any" title="Any"
                                          onclick="{!c.checkBoolean}">&nbsp;ANY &nbsp;&nbsp;&nbsp;&nbsp;</span>
                                    <span style="color:black" data-id="{!masterindex}" data-value="Custom"
                                          onclick="{!c.checkBoolean}">
                                        <ui:inputRadio value="{!tableList[0].customCondition}" />
                                    </span>
                                    <span data-id="{!masterindex}" data-value="Custom" title="Custom"
                                          onclick="{!c.checkBoolean}">&nbsp;CUSTOM</span>
                                </span>
                            </div>
                        </div>
