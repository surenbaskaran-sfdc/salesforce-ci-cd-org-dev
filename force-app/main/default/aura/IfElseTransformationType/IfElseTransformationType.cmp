                                    <tr class="slds-text-title_caps">
                                        <th scope="col">
                                            <div class="slds-align_absolute-center " title="sl.No">Sl.No</div>
                                        </th>
                                        <th scope="col">
                                            <div class="slds-align_absolute-center " title="field">
                                                <p class="required">*</p> FIELD
                                                
                                            </div>
                                        </th>
                                        <th scope="col">
                                            <div class="slds-align_absolute-center " title="operator">
                                                <p class="required">*</p> OPERATOR
                                                
                                            </div>
                                        </th>
                                        <th scope="col">
                                            <div class="slds-align_absolute-center " title="type">
                                                <p class="required">*</p> TYPE
                                                
                                            </div>
                                        </th>
                                        <th scope="col">
                                            <div class=" slds-align_absolute-center" title="value">
                                                <p class="required">*</p> VALUE
                                                
                                            </div>
                                        </th>
                                        <th scope="col"></th>
                                        <th scope="col">
                                            <div class="slds-align_absolute-center " title="Delete">Delete</div>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <aura:iteration items="{!tableList}" var="set" indexVar="childindex">
                                        <tr>
                                            <td>
                                                <span class="slds-align_absolute-center" title="{!childindex + 1}"> {!childindex + 1}</span>
                                            </td>
                                            <td>
                                                <div style="color:black" data-id="{!masterindex}" data-value="{!childindex}"
                                                     onchange="{!c.onHeaderChange}">
                                                    <lightning:combobox aura:id="select_Field" class="{!set.fieldError}"
                                                                        name="FieldReference" label="FieldReference" placeholder="--None--"
                                                                        variant="label-hidden" value="{!set.field}"
                                                                        options="{! v.headerOptions }" />
                                                </div>
                                            </td>
                                            <td>
                                                <div style="color:black" data-id="{!masterindex}" data-value="{!childindex}"
                                                     data-record="{!set.operator}" onchange="{!c.onOperatorChange}">
                                                    <lightning:combobox variant="label-hidden" class="{!set.operatorError}"
                                                                        value="{!set.operator}" disabled="{! not(set.fieldpresent)}"
                                                                        options="{! v.textoperators }" placeholder="--None--" />
                                                </div>
                                            </td>
                                            <td>
                                                <aura:if isTrue="{!set.isNullOperator}">
                                                    <div style="color:black" data-id="{!masterindex}" data-value="{!childindex}"
                                                         onchange="{!c.onTypeChange}">
                                                        <lightning:combobox variant="label-hidden" class="{!set.typeError}"
                                                                            style="color:black" value="{!set.type}" placeholder="--None--"
                                                                            disabled="{! not(set.fieldpresent)}" options="{! v.booleantype }" />
                                                    </div>
                                                    <aura:set attribute="else">
                                                        <div style="color:black" data-id="{!masterindex}"
                                                             data-value="{!childindex}" onchange="{!c.onTypeChange}">
                                                            <lightning:combobox variant="label-hidden" class="{!set.typeError}"
                                                                                style="color:black" value="{!set.type}"
                                                                                disabled="{! not(set.fieldpresent)}" options="{! v.texttype }"
                                                                                placeholder="--None--" />
                                                        </div>
                                                    </aura:set>
                                                </aura:if>
                                            </td>
                                            <aura:if isTrue="{!set.isString}">
                                                <td>
                                                    <lightning:input variant="label-hidden" class="{!set.valueError}"
                                                                     disabled="{! not(set.isTypepresent)}" value="{!set.value}" />
                                                </td>
                                                <td></td>
                                            </aura:if>
                                            <aura:if isTrue="{!set.isBoolean}">
                                                <td>
                                                    <lightning:combobox variant="label-hidden" class="{!set.valueError}"
                                                                        style="color:black" value="{!set.value}" placeholder="--None--"
                                                                        disabled="{! not(set.isTypepresent)}" options="{! v.booleanoptions }" />
                                                </td>
                                                <td></td>
                                            </aura:if>
                                            <aura:if isTrue="{!set.isReference}">
                                                <td>
                                                    <lightning:combobox name="FieldReference" class="{!set.valueError}"
                                                                        label="FieldReference" placeholder="--None--" variant="label-hidden"
                                                                        value="{!set.value}" options="{! v.headerValueOptions }"  />
                                                </td>
                                                <td></td>
                                            </aura:if>
                                            <td>
                                                <div data-id="{!masterindex}" data-value="{!childindex}"
