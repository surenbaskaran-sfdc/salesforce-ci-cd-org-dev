                                </div>
                            </aura:if>
                            <aura:if isTrue="{!tableList[0].thenReferece}">
                                <div class="slds-col slds-p-left_medium  slds-size_3-of-12  " data-id="{!masterindex}"
                                     onchange="{!c.onThenValueChange}">
                                    <lightning:select class="{!tableList[0].thenValueError}" value="{!tableList[0].thenvalue}">
                                        <option text="--None--" value="" />
                                        <aura:iteration items="{!v.thenAndElseOptions}" var="option">
                                            <option text="{!option.label}" value="{!option.value}" />
                                        </aura:iteration>
                                    </lightning:select>
                                </div>
                            </aura:if>
                        </div>
                        <br></br>
                    </div>
                </div>
                <br />
            </aura:iteration>
