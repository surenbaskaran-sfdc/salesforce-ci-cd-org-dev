                                                    <lightning:icon class="slds-align_absolute-center" iconName="utility:delete"
                                                                    size="x-small" alternativeText="Delete" />
                                                    <span class="slds-assistive-text">Delete</span>
                                                </div>
                                            </td>
                                        </tr>
                                    </aura:iteration>
                                </tbody>
